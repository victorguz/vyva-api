    "crypto-js": "^4.2.0",
    "dynamoose": "^4.0.4",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "nestjs-dynamoose": "^0.6.0",
    "reflect-metadata": "^0.1.14",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  },
